<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.phoebevu.gorepresent.CongressView"
    tools:showIn="@layout/activity_congress_view">


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/rep1"
        android:src="@drawable/barbara"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:onClick="rep1"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Barbara Boxer"
        android:id="@+id/textView"
        android:padding="2dp"
        android:textSize="12dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_alignTop="@+id/rep1"
        android:layout_toEndOf="@+id/rep1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email: Sen.Boxer@opencongress.org"
        android:id="@+id/textView2"
        android:layout_below="@+id/textView"
        android:layout_alignStart="@+id/textView"
        android:textSize="12dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Web: http://www.boxer.senate.gov"
        android:id="@+id/textView3"
        android:layout_below="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:textSize="12dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Putting the country first means Obama nominating a  justice and the Senate..."
        android:id="@+id/textView4"
        android:textSize="12dp"
        android:layout_below="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton"
        android:layout_below="@+id/rep2"
        android:layout_alignParentStart="true"
        android:layout_marginTop="75dp"
        />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/rep2"
        android:layout_marginTop="57dp"
        android:src="@drawable/dianne"
        android:layout_below="@+id/rep1"
        android:layout_alignParentStart="true"
        android:onClick="rep2"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dianne Feinstein"
        android:id="@+id/textView5"
        android:layout_alignTop="@+id/rep2"
        android:layout_alignStart="@+id/textView4"
        android:layout_margin="5dp"
        android:textSize="12dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email: Sen.Feinstein@opencongress.org"
        android:id="@+id/textView7"
        android:textSize="12dp"
        android:layout_below="@+id/textView5"
        android:layout_alignStart="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Web: http://www.feinstein.senate.gov"
        android:id="@+id/textView8"
        android:textSize="12dp"
        android:layout_below="@+id/textView7"
        android:layout_alignStart="@+id/textView5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Today we reflect on the contribution and leadership of the US presidents through out our great ..."
        android:id="@+id/textView9"
        android:textSize="12dp"
        android:layout_above="@+id/rep3"
        android:layout_alignStart="@+id/textView8" />



    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/rep3"
        android:src="@drawable/lorretta"
        android:layout_below="@+id/rep2"
        android:layout_alignParentStart="true"
        android:layout_marginTop="50dp"
        android:onClick="rep3"
        />



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lorretta Sanchez"
        android:id="@+id/textView6"
        android:textSize="12dp"
        android:layout_alignTop="@+id/rep3"
        android:layout_alignStart="@+id/textView10" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email: Rep.LorrettaSanchez@opencongress.org"
        android:id="@+id/textView10"
        android:textSize="12dp"
        android:layout_alignTop="@+id/imageButton"
        android:layout_alignStart="@+id/textView11" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Web: http://www.lorrettasanchez.house.gov"
        android:id="@+id/textView11"
        android:textSize="12dp"
        android:layout_below="@+id/textView10"
        android:layout_alignStart="@+id/textView4" />






</RelativeLayout>
